         <top>
            <ImageView fitWidth="600.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="-70.0" top="-30.0" />
               </BorderPane.margin>
               <image>
                  <Image url="@../pics/newTitle.png" />
               </image>
            </ImageView>
         </top>
         <bottom>
            <ImageView fitHeight="250.0" fitWidth="2000.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
               <image>
                  <Image url="@../pics/podium.png" />
               </image>
               <BorderPane.margin>
                  <Insets bottom="100.0" />
